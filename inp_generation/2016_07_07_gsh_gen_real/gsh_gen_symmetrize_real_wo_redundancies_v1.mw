<Input>
<Text-field prompt="&gt; " style="Maple Input" bold="false" layout="Normal"><Font bold="true" style="2D Input"># this section runs through the indices in l_set, gets the symmetry coefficients, calls generate_T_Function to get the
# symmetrized basis functions, gets the l, m and n indices associated with the basis function and puts the
# the indices and functions into a nice array format</Font><Font style="2D Input">
genTFunction := proc(l_set)
local tfunction_index, tfunction_final, index_l, symcoef, index_m, temp_final, temp_index, str_index_l, str_index_m, temp_out, coeff_temp, coeff_final, index_n, n_vals, index_final, c;
tfunction_index := []:
tfunction_final := [];
index_final := [];
c = 0:

<Font bold="true"># Iterate over L values</Font>
for index_l in l_set
do

<Font bold="true"># Get Symmetry Coefficients...</Font>
symcoef := independent_sols(index_l):

if symcoef = false then
next:
end if:

#for index_m from -index_l to index_l
for index_m from 0 to index_l
do

temp_final, temp_index := generate_T_Function(index_l,index_m,symcoef):

temp_index := Array(temp_index);

#print(temp_index);

# temp_final := Array(temp_final);
temp_final := Array((2*index_l+1)*temp_final);

#n_vals := temp_index(1..-1,3);
n_vals := temp_index(.., 3);

for index_n from 1 to ArrayNumElems(n_vals)
do

# print(temp_index(index_n, ..)):
# print(temp_final(index_n)):

tfunction_final := [op(tfunction_final),[simplify(temp_final(index_n))]]:
index_final := [op(index_final), [temp_index(index_n,1..-1)]]:

end do:
end do:
print(cat(&quot;l = &quot;, index_l, &quot; analysis completed&quot;)):
end do: 

return tfunction_final, index_final:

end proc:</Font><Font bold="false">
</Font></Text-field>
</Input>
</Group>
