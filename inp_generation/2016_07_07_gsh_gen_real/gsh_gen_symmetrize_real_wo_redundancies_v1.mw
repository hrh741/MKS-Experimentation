<Group view="code" labelreference="L594" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="2D Input" layout="Normal"><Font bold="true"># note: in the following procedure m, n and s only iterate from 0 to l because for the real version of T
# T(phi1,phi,phi2,l,m,n) = T(phi1,phi,phi2,l,-m,n) = T(phi1,phi,phi2,l,m,-n)</Font>
independent_sols := proc(index_l)

global pmat, eqnelement, getIndexed;
local l, p, free_parameters, ACoeffs, idx_eqn, weight, idx_inner, temp_sol, idx_A_Coefficient, ACoeffsT, orthoA, st, timeE, numn;
<Font bold="true">
# p has shape nsym*len(-l..l) by len(-1..l)
</Font>p := pmat(index_l):
#print(evalf(p)):

<Font bold="true"># Solve equation 14.115 in Bunge's book</Font>
l := Matrix(LinearSolve(p, ZeroVector(Dimension(p)[1]),free=t)):
#print(l):

free_parameters := convert(convert(map(getIndexed, convert(l,list)),set) minus {1=1},list):
#print(free_parameters):

<Font bold="true"># <Font executable="false">Is there at least one non-trivial solution?</Font></Font>
if nops(free_parameters) &lt; 1 and MatrixNorm(l,2) = 0 then
return false:
end if:

<Font bold="true"># Initialize the matrix of coefficeints that satisfy equation 14.115</Font>
ACoeffs := Matrix((2*index_l+1), nops(free_parameters), (i, j) -&gt; A[i, j]):
#ACoeffs := Matrix((index_l+1), nops(free_parameters), (i, j) -&gt; A[i, j]):

# <Font bold="true" executable="false">Enumerate over all linearly independent solutions...  (fill in the ACoeff matrix)</Font>
for idx_eqn from 1 to nops(free_parameters)
do

weight := [free_parameters[idx_eqn]=1]:

for idx_inner from 1 to nops(free_parameters)
do

if not (idx_eqn=idx_inner) then

weight := [op(weight),free_parameters[idx_inner]=0];

end if:

end do:

temp_sol := eval(l, weight);

for idx_A_Coefficient from 1 to 2*index_l+1
#for idx_A_Coefficient from 1 to index_l+1
do
ACoeffs[idx_A_Coefficient,idx_eqn] := temp_sol[idx_A_Coefficient,1]:
end do:

end do:

<Font bold="true" executable="false">Make the solutions mutually orthogonal</Font><Font executable="false">
</Font><Equation executable="true" style="2D Input" input-equation="numn := Size(ACoeffs)[2]:" display=""></Equation>

return Matrix(ACoeffs):
end proc:</Text-field>
</Input>
</Group>
