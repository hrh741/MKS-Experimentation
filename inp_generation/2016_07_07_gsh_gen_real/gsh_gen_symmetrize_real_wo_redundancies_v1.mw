<Text-field prompt="&gt; " style="2D Input" layout="Normal">generate_T_Function_old := proc(index_l, index_m, symcoef)
<Font bold="true"># for a choice of the l and m indices use the symmetry coefficients to generate the symmetrized and orthogonal
# GSH basis functions</Font>

local verifyMat, SymmetrizedT, count_solutions, out_index, out_t_function, nrow, ncol, orthoA, symcoef_:

if index_m = 0 then
verifyMat := (l, m) -&gt; [[seq(Talt(phi1, phi, phi2, l, m, n), n=0..l)]]:
<Font bold="true"># we also have to perform GS orthonormalization again on the reduced set of coefficients </Font>
nrow := Size(symcoef)[1]:
ncol := Size(symcoef)[2]:
symcoef_ := symcoef[MTM[int16](ceil(nrow/2))..-1,..]:
orthoA := LinearAlgebra[GramSchmidt]([seq(symcoef_[.., ii], ii = 1 .. ncol)], normalized = true);
symcoef_ := Matrix(Concatenate(2, orthoA));
else
verifyMat := (l, m) -&gt; [[seq(Talt(phi1, phi, phi2, l, m, n), n=-l..l)]]:
symcoef_:=symcoef:
end if:

#print(symcoef):
#print(symcoef_):

SymmetrizedT := Matrix(verifyMat(index_l,index_m)). symcoef_:

count_solutions := Dimension(SymmetrizedT)[2]:
out_index := seq([index_l,index_m,index_nu],index_nu=1..count_solutions):
out_t_function := convert(transpose(Matrix([SymmetrizedT])),listlist):

return out_t_function, [out_index]:

end proc:
</Text-field>
</Input>
</Group>
